{
  "name": "num-agents",
  "version": "3.0.1",
  "description": "Production-ready Multi-Agent Control Plane with Gemini API integration, Flow DAG execution, and comprehensive dashboard.",
  "type": "module",
  "license": "PRIVATE",
  "author": {
    "name": "Numtema Foundry AI",
    "email": "numtemalionel@gmail.com",
    "url": "https://numtema.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Creativityliberty/num.git"
  },
  "bin": {
    "mcp-agents-modes": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc -p tsconfig.json && npm run copy:templates",
    "copy:templates": "mkdir -p dist/src/dashboard/templates && cp -r src/dashboard/templates/* dist/src/dashboard/templates/",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "vitest run",
    "ci": "npm run typecheck && npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "fast-glob": "^3.3.2",
    "js-yaml": "^4.1.0",
    "pino": "^9.5.0",
    "ws": "^8.18.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vitest": "^2.1.8"
  }
}
